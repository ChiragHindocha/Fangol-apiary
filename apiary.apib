    + limit: 10
    + sort: "najlepsze"
    + type: ["fakt", "foto"]
    + tags: ["MESSI", "RONALDO"]
    + q: "wczoraj"
    + user_id: 1
    + contest_id: 1
    + queue: waiting
    + timeframe: lastday
    
